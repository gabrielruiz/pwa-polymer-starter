{
  "hosting": {
    "public": "build/es6-bundled/",
    "rewrites": [
      {
        "source": "**/!(*.*)",
        "destination": "/index.html"
      }
    ],
    "headers": [
     { 
       "source":"/service-worker.js", 
       "headers": [
         {
           "key": "Cache-Control", 
           "value": "no-cache"
         }
       ] 
     }
   ]
  }
}
